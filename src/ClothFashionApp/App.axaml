<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:ClothFashionApp"
             x:Class="ClothFashionApp.App"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme />
		<StyleInclude Source="avares://ClothFashionApp/Controls/VirtualizedGrid.axaml" />
    </Application.Styles>
	
	<Application.Resources>
		
		<FontFamily x:Key="BebasFont">avares://ClothFashionApp/Assets/#Bebas</FontFamily>

	</Application.Resources>
	
</Application>